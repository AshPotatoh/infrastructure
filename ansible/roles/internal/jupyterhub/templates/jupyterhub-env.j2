JUPYTERHUB={{ jupyterhub_srv_dir }}

{% if jupyterhub_oauth_github_host is defined %}
GITHUB_HOST={{ jupyterhub_oauth_github_host }}
{% endif %}
{% if jupyterhub_oauth_client_id is defined %}
OAUTH_CLIENT_ID={{ jupyterhub_oauth_client_id }}
{% endif %}
{% if jupyterhub_oauth_client_secret is defined %}
OAUTH_CLIENT_SECRET={{ jupyterhub_oauth_client_secret }}
{% endif %}
{% if jupyterhub_oauth_callback_url is defined %}
OAUTH_CALLBACK_URL={{ jupyterhub_oauth_callback_url }}
{% endif %}
